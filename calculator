a=int(input())
b=iint(input())
print("1.ADD\n2.SUB\n 3.MULTI\n4.DIVIDE\n5.MODULo\n6.EXPONENT\n")
c=int(input())
if(c==1):
add=a+b
print("ADD=",add)
elif(c==2):
sub=a-b
print("SUB=",sub)
elif(c==3):
mul=a*b
print("MUL=",mul)
elif(c==4):
div=a/b
print("DIV=",div)
elif(c==5):
modulo=a%b
print("MODULO=",modulo)
elif(c==6):
exp=a**b
print("EXPONENT=",exp)
else:
print("Choice is invalid)
